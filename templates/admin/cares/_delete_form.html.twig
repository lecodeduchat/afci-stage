<form method="post" action="{{ path('cares_delete', {'id': care.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette séance ??');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ care.id) }}">
    <button class="btn-trash"><i class="fa-solid fa-trash-can"></i></button>
</form>
