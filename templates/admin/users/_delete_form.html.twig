<form method="post" action="{{ path('users_delete', {'id': user.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer ce patient ? ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn-trash"><i class="fa-solid fa-trash-can"></i></button>
</form>
