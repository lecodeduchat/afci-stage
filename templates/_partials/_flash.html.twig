{% for label,messages in app.flashes %}
    {% for message in messages %}
           <div class="{{label}}" role="alert">
				<div class="alert-message">
				 {{message|raw}}
				</div>
				<button  class="alertClose" aria-label="close"><i class="fa-solid fa-xmark"></i></button>
			</div> 
    {% endfor %}
    
{% endfor %}